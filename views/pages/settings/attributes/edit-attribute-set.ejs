<% include ../../../partials/header %>
<div class="row">
  <div class="col-md-12">
    <div class="x_panel">
      <div class="x_title">
        <h2>Edit Attribute Set<small></small></h2>
        <ul class="nav navbar-right panel_toolbox">
          <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a></li>
        </ul>
        <div class="clearfix"></div>
      </div>
      <div class="x_content">
        <form action="/settings/add-attribute-set/<%= attrib.id %>" method="post">
        <div class="row">
          <div class="col-md-4">
            <div class="form-group">
              <label class="control-label col-md-3 col-sm-3 col-xs-12">Attribute:</label>
              <div class="col-md-9 col-sm-9 col-xs-12">
                <label><%= attrib.title %></label>
              </div>
            </div>
          </div>
          <div class="col-md-4">
            <h4>Assigned Entities</h4>
            <hr/>
            <div id="assigned" class="form-group">
                <% for (var i = 0; i < attEnt.length; i++){ %>
                    <div class="checkbox">
                        <label>
                        <input type="checkbox" class="flat" name="entity[]" value="<%= attEnt[i].entities.id %>" onclick="removeval(this)" id="<%= attEnt[i].entities.title %>" checked> <%= attEnt[i].entities.title %>
                        </label>
                    </div>
                <% } %>
          </div>
          </div>
          <div class="col-md-4"> 
            <h4>Unassigned Entities</h4>
            <hr/>
            <div id="unassigned" class="form-group">
            <% for (var i = 0; i < entities.length; i++){ %>
              <div class="checkbox">
                <label>
                  <input type="checkbox" class="flat" name="entity[]" value="<%= entities[i].id %>" onclick="addval(this)" id="<%= entities[i].title %>"> <%= entities[i].title %>
                </label>
              </div>
            <% } %>
          </div>
          </div>
        </div>

          <div class="row">
            <div class="form-actions">
              <button type="submit" class="btn btn-primary">Submit</button>
            </div>
          </div>
        </form>
      </div>
    </div>
  </div>
</div>
<% include ../../../partials/footer %>
